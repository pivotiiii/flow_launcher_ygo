{
    "name": "ygo-card-search",
    "version": "1.2.0",
    "description": "Plugin for Flow Launcher that searches for Yu-Gi-Oh! cards on ygoprodeck.com",
    "main": "./dist/main.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/pivotiiii/flow_launcher_ygo.git"
    },
    "keywords": [
        "flowlauncher",
        "yugioh",
        "card game"
    ],
    "author": {
        "name": "pivotiiii",
        "url": "https://pivotiiii.github.io"
    },
    "bugs": {
        "url": "https://github.com/pivotiiii/flow_launcher_ygo/issues"
    },
    "homepage": "https://github.com/pivotiiii/flow_launcher_ygo",
    "engines": {
        "node": ">=10.12.0"
    },
    "type": "module",
    "scripts": {
        "build": "esbuild src/main.ts --bundle --platform=node --format=esm --outfile=dist/main.js --external:sharp --external:open --external:win-screen-resolution --external:node-fetch",
        "test": "jest",
        "start": "node dist/main.js"
    },
    "dependencies": {
        "node-fetch": "^3.3.2",
        "open": "9.1.0",
        "sharp": "0.32.6",
        "win-screen-resolution": "3.0.2"
    },
    "devDependencies": {
        "@types/jest": "^29.5.0",
        "@types/node": "^25.0.9",
        "esbuild": "^0.27.2",
        "jest": "^29.7.0",
        "ts-jest": "^29.1.0",
        "typescript": "^5.9.3"
    }
}
